{
  "luci-app-fakehttp": {
    "description": "Grant UCI access for luci-app-fakehttp",
    "read": {
      "file": {
        "/var/run/fakehttp/*": [ "read" ]
      },
      "ubus": {
        "service": [ "list" ]
      },
      "uci": [ "fakehttp" ]
    },
    "write": {
      "uci": [ "fakehttp" ]
    }
  }
} 